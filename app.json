{
  "expo": {
    "name": "Tic Tac Toe 2025",
    "slug": "tiktok2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hmyousuf2010.tictactoe2025",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/adaptiveIcon.png",
        "monochromeImage": "./assets/images/logo/adaptiveIcon.png",
        "backgroundColor": "#f0f0f0"
      },
      "package": "com.hmyousuf2010.tictactoe2025"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/adaptiveIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f0f0f0",
          "dark": {
            "image": "./assets/images/logo/adaptiveIconDark.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "assetExts": ["svg"]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eb9966d1-cb29-4ea2-b35e-98fddb166d16"
      }
    },
    "owner": "hmyousuf2010"
  }
}
